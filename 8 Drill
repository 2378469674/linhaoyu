void fibonacci(int x,int y,vector<int>&v,int n)
 {
     v.push_back(x);
     v.push_back(y);
      int z;
      for(int i=0;i<n-2;i++)
     {
         z=x+y;
         if(z<0)
         {
             cout<<y<<endl;
             break;
         }
         v.push_back(z);
         x=y;
         y=z;
     }
  }
